{
  "window.title": " ",

  "zenMode.hideLineNumbers": false,
  "zenMode.fullScreen": false,
  "zenMode.hideStatusBar": false,

  "workbench.activityBar.visible": false,
  "workbench.layoutControl.type": "menu",
  "workbench.sideBar.location": "right",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.startupEditor": "none",
  "workbench.layoutControl.enabled": false,
  "workbench.editor.showTabs": false,
  "workbench.tree.renderIndentGuides": "none",

  "files.autoSave": "onWindowChange",
  "files.insertFinalNewline": false,
  "files.trimFinalNewlines": false,
  "files.trimTrailingWhitespace": true,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/env/**": true,
    "**/venv/**": true,
    "env-*": true
  },

  "editor.fontFamily": "JetBrainsMono Nerd Font Mono",
  "editor.fontSize": 14,
  "editor.lineHeight": 22,
  "editor.fontWeight": 400,
  "editor.fontLigatures": true,
  "editor.codeActionsOnSave": {
    "source.addMissingImports": true,
    "source.organizeImports": true
  },
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "comment",
          "entity.name.type.class", //class namefs
          "constant", //String, Number, Boolean…, this, super
          "storage.modifier", //static keyword
          "storage.type.class.js", //class keyword
          "keyword" //import, export, return…
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": [
          "invalid",
          "keyword.operator",
          "constant.numeric.css",
          "keyword.other.unit.px.css",
          "constant.numeric.decimal.js",
          "constant.numeric.json"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      }
    ]
  },
  "editor.scrollbar.horizontal": "hidden",
  "editor.occurrencesHighlight": false,
  "editor.suggest.insertMode": "replace",
  "editor.guides.indentation": false,
  "editor.cursorStyle": "block",
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.stickyScroll.enabled": true,
  "editor.renderWhitespace": "none",
  "editor.lineNumbers": "relative",
  "editor.rulers": [80],
  "editor.minimap.enabled": false,
  "editor.matchBrackets": "never",
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },

  "prettier.endOfLine": "lf",
  "prettier.tabWidth": 2,
  "prettier.singleQuote": true,
  "prettier.semi": false,
  "prettier.useTabs": false,

  "vscode_custom_css.imports": ["file:///Users/shendypratama/.config/vscode-font-style/.vscode-style.css"],

  "projectManager.git.baseFolders": ["$home/Public/project/typescript"],
  "projectManager.sortList": "Recent",

  "advancedNewFile.exclude": {
    "node_modules": true,
    "node_modules_electron": true,
    "dev": true,
    "dist": true
  },
  "advancedNewFile.showInformationMessages": true,
  "advancedNewFile.convenienceOptions": ["last", "current", "root"],

  "code-eol.color": "#D0D3D9",
  "importCost.bundleSizeColoring": "compressed",
  "chatgpt.lang": "en",

  "jsdoc-generator.includeTime": false,
  "jsdoc-generator.includeDate": false,

  "vim.statusBarColorControl": false,
  "vim.statusBarColors.normal": ["#8FBCBB", "#000"],
  "vim.statusBarColors.insert": ["#BF616A", "#000"],
  "vim.statusBarColors.visual": ["#B48EAD", "#000"],
  "vim.statusBarColors.visualline": ["#B48EAD", "#000"],
  "vim.statusBarColors.visualblock": ["#A3BE8C", "#000"],
  "vim.statusBarColors.replace": "#D08770",
  "vim.statusBarColors.commandlineinprogress": "#007ACC",
  "vim.statusBarColors.searchinprogressmode": "#007ACC",
  "vim.statusBarColors.easymotionmode": "#007ACC",
  "vim.statusBarColors.easymotioninputmode": "#007ACC",
  "vim.statusBarColors.surroundinputmode": "#007ACC",
  "vim.easymotion": true,
  "vim.easymotionMarkerBackgroundColor": "#424874",
  "vim.easymotionMarkerForegroundColorOneChar": "#F4EEFF",
  "vim.easymotionMarkerForegroundColorTwoCharFirst": "#F4EEFF",
  "vim.easymotionMarkerForegroundColorTwoCharSecond": "#F4EEFF",
  "vim.easymotionMarkerFontWeight": "bold",
  "vim.changeWordIncludesWhitespace": false,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": false,
  "vim.highlightedyank.enable": true,
  "vim.highlightedyank.duration": 50,
  "vim.history": 1,
  "vim.surround": true,
  "vim.highlightedyank.color": "pink",
  "vim.highlightedyank.textColor": "white",
  "vim.leader": "<space>",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [" "],
      "after": ["leader", "leader", "leader", "b", "d", "w"]
    }
  ],
  "vim.handleKeys": {
    "<C-d>": true,
    "<C-u>": true
  },
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "k"],
      "after": ["<Esc>"]
    }
  ],
  "vim.normalModeKeyBindings": [
    {
      "before": ["<S-h>"],
      "commands": [":tabp"]
    },
    {
      "before": ["<S-l>"],
      "commands": [":tabn"]
    },
    {
      "before": ["<C-r>"],
      "commands": [":nohl"]
    },
    {
      "before": ["u"],
      "commands": [":undo"]
    },
    {
      "before": ["]", "q"],
      "commands": [":wq"]
    },
    {
      "before": ["]", "g", "n"],
      "commands": ["jsdoc-generator.generateJsdoc"]
    },
    {
      "before": ["<S-k>"],
      "commands": ["editor.action.showHover"]
    },
    {
      "before": ["]", "d"],
      "commands": ["editor.action.marker.next"]
    },
    {
      "before": ["]", "c", "s"],
      "commands": ["workbench.action.clearEditorHistory"]
    },
    {
      "before": ["[", "d"],
      "commands": ["editor.action.marker.prev"]
    },
    {
      "before": ["]", "c", "a"],
      "commands": ["editor.action.quickFix"]
    },
    {
      "before": ["]", "w"],
      "commands": ["workbench.action.files.save"]
    },
    {
      "before": ["]", "r", "n"],
      "commands": ["workbench.action.removeRootFolder"]
    },
    {
      "before": ["s", "p"],
      "commands": ["workbench.action.splitEditor"]
    },
    {
      "before": ["v", "s", "p"],
      "commands": ["workbench.action.splitEditorDown"]
    },
    {
      "before": ["]", "f"],
      "commands": ["workbench.action.showCommands"]
    },
    {
      "before": ["]", "z"],
      "commands": ["workbench.action.toggleZenMode"]
    },
    {
      "before": ["<c-u>"],
      "after": ["2", "0", "k", "z", "z"]
    },
    {
      "before": ["<c-d>"],
      "after": ["2", "0", "j", "z", "z"]
    }
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": [">"],
      "after": [">", "g", "v"]
    },
    {
      "before": ["<"],
      "after": ["<", "g", "v"]
    },
    {
      "before": [],
      "after": []
    }
  ],

  "search.quickOpen.includeHistory": false,
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/env": true,
    "**/venv": true
  },

  "telemetry.telemetryLevel": "off",
  "terminal.integrated.defaultProfile.osx": "tmux-shell",
  "terminal.integrated.profiles.osx": {
    "tmux-shell": {
      "path": "tmux",
      "args": ["new-session", "-A"]
    }
  },
  "git.autofetch": true,
  "diffEditor.renderSideBySide": false,
  "workbench.colorTheme": "Catppuccin Macchiato"
}
